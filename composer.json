{
	"name": "roman3349/iqrf-cloud-nette",
	"description": "IQRF Cloud API wrapper for Nette Framework.",
	"keywords": ["iqrf", "cloud", "api", "nette"],
	"type": "library",
	"homepage": "https://github.com/Roman3349/iqrf-cloud-nette",
	"license": ["GPL-3.0"],
	"authors": [
		{
			"name": "Roman Ondráček",
			"homepage": "https://github.com/Roman3349",
			"email": "ondracek.roman@centrum.cz",
			"role": "developer"
		}
	],
	"require": {
		"php": ">= 5.4",
		"guzzlehttp/guzzle": "^6.1",
		"nette/bootstrap": "^2.3",
		"nette/di": "^2.3",
		"nette/robot-loader": "^2.3",
		"nette/utils": "^2.3"
	},
	"require-dev": {
		"nette/tester": "^1.7",
		"satooshi/php-coveralls": "^1.0",
		"mockery/mockery": "^0.9.4"
	},
	"support": {
		"email": "ondracek.roman@centrum.cz",
		"issues": "https://github.com/Roman3349/iqrf-cloud-nette/issues"
	},
	"autoload": {
		"psr-4": {
			"IQRF\\Cloud\\": "src/"
		}
	},
	"config": {
		"vendor-dir": "vendor"
	},
	"archive": {
		"exclude": ["vendor", "tests", ".travis.yml"]
	}
}
